#include <stdio.h>
#include <stdlib.h>

int main()

{
    int a,b,r;
   printf("Donner le premier nombre : "); scanf("%d", &a); printf("Donner le deuxieme nombre: ");

scanf("%d", &b);

// Algorithme d'Euclide

while (b != 0) {

}

r = a % b;

a = b;

b = r;

// Affichage du r√©sultat

printf("Le PGCD est: %d\n", a);
    return 0;
}
